        position: relative;
        margin-left: 5%;
        margin-top: 5%;
        display: flex;
        align-items: center;
        justify-content: center;
      }
      a {
        text-decoration: none;
      }
      .reg2 {
        color: blue;
      }
      .card {
        height: 325px;
        padding: 5px;
        margin-left: 2px;
        width: 300px;
        margin: 0%;
        border-radius: 5px;
      }
      img {
        border-radius: 5px;
        margin: 0%;
      }
      .cardpard {
